{
	"applications": {
	  "gecko": {
		"id": "spotify-ad-muter@juliocesar.me",
		"strict_min_version": "57.0"
	  }
	},
	"content_scripts": [{
		"all_frames": true,
		"exclude_globs": [],
		"include_globs": ["*open.spotify.com*"],
		"js": ["includes/jquery-3.7.0.min.js", "includes/jquery.hotkeys.js", "includes/spotify.js"],
		"matches": ["*://open.spotify.com/*"],
		"run_at": "document_start"
	}],
	"description": "Automatically mute audio ads.",
	"icons": {
		"128": "icon.png" 
	},   
	"manifest_version": 2,
	"name": "Spotify Ad Auto-Muter",
	"short_name": "Spotify Ad Auto-Muter",	
	"options_ui": {
		"page": "options.html"
	},
	"permissions": [
		"storage",		
		"https://www.google-analytics.com/"
	],
	"version": "0.1.3",
	"web_accessible_resources": ["includes/jquery-3.7.0.min.js", "includes/jquery.hotkeys.js", "includes/spotify.js", "jquery-3.7.0.min.map" ]	
}
